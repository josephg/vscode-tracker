{
  "name": "operationtracker",
	"displayName": "Operation Tracker",
	"description": "Record local editing operations into a file",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.48.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"publisher": "josephgentle",
	"main": "./out/extension.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/josephg/vscode-tracker"
	},
	"contributes": {
		"commands": [
			{
				"command": "operationtracker.helloWorld",
				"title": "Hello World"
			}
		],
		"configuration": {
			"title": "Operation Tracker",
			"properties": {
				"operationtracker.outputDirectory": {
					"type": "string",
					"default": "~/vscodelogs",
					"description": "Directory to put JSON log files"
				},
				"operationtracker.storeContents": {
					"type": "boolean",
					"description": "Store file contents on open and close",
					"default": true
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.48.0",
		"@types/mocha": "^8.0.0",
		"@types/node": "^14.0.27",
		"mocha": "^8.1.3",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0"
	}
}
